{
    "name": "grafite/maintenance",
    "description": "A handy set of commands for app maintenance",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3|>=8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "mikehaertl/php-shellcommand": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\Maintenance\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\Maintenance\\GrafiteMaintenanceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
